authoritative;
default-lease-time 600;
max-lease-time 600;

subnet {{ dhcpd_subnet }} netmask {{ dhcpd_netmask }} {
    range {{ dhcpd_start }} {{ dhcpd_end }};

    option subnet-mask 255.255.0.0;
    option domain-name "{{ dhcpd_domain }}";
    option routers {{ ip4 }};
    option domain-name-servers {{ ip4 }};
}
